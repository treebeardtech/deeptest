{
	"name": "deeptest",
	"displayName": "deeptest",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:deeptest.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "deeptest.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"format": "prettier --write **/*.ts",
		"format-check": "prettier --check **/*.ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.17.0",
		"eslint": "^7.19.0",
		"eslint-plugin-prettier": "^3.3.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
